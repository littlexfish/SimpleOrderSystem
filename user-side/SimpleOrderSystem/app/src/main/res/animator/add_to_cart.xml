        android:duration="100"
        android:valueType="floatType"
        android:valueTo="6" />

    <objectAnimator android:propertyName="rotation"
        android:duration="100"
        android:valueType="floatType"
        android:valueTo="-6" />

    <objectAnimator android:propertyName="rotation"
        android:duration="100"
        android:valueType="floatType"
        android:valueTo="5" />

    <objectAnimator android:propertyName="rotation"
        android:duration="100"
        android:valueType="floatType"
        android:valueTo="-5" />

    <objectAnimator android:propertyName="rotation"
        android:duration="100"
        android:valueType="floatType"
        android:valueTo="4" />

    <objectAnimator android:propertyName="rotation"
        android:duration="100"
        android:valueType="floatType"
        android:valueTo="-4" />

    <objectAnimator android:propertyName="rotation"
        android:duration="100"
        android:valueType="floatType"
        android:valueTo="3" />

    <objectAnimator android:propertyName="rotation"
        android:duration="100"
        android:valueType="floatType"
        android:valueTo="-3" />

    <objectAnimator android:propertyName="rotation"
